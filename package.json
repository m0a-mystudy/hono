{
  "name": "hono-drizzle-todo",
  "version": "1.0.0",
  "description": "Todo API with Hono and Drizzle",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "wrangler dev",
    "build": "tsc",
    "generate": "drizzle-kit generate:sqlite",
    "deploy": "wrangler deploy",
    "migration-remote": "npx wrangler d1 migrations apply todo-db --remote",
    "migration-local": "npx wrangler d1 migrations apply todo-db --local",
    "dev:client": "vite",
    "build:client": "vite build && bun run scripts/generate-manifest.ts",
    "preview": "vite preview"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.69.0",
    "@trpc/client": "^11.0.0",
    "@trpc/react-query": "^11.0.0",
    "@trpc/server": "^11.0.0",
    "drizzle-orm": "^0.29.5",
    "hono": "^4.1.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240208.0",
    "@types/node": "^20.11.28",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^4.2.0",
    "drizzle-kit": "^0.30.5",
    "typescript": "^5.4.2",
    "wrangler": "^4.4.0"
  }
}
